                    </androidx.constraintlayout.widget.ConstraintLayout>-->

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp">

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerview"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_stroke2"
                            android:minHeight="200dp"
                            android:overScrollMode="never"
                            android:padding="5dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"

                            app:layout_constraintTop_toTopOf="parent"
                            tools:itemCount="4"
                            tools:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                            tools:listitem="@layout/item_view_dashboard"
                            tools:spanCount="2" />

                        <ImageView
                            android:id="@+id/retryBtn"
                            android:layout_width="36dp"
                            android:layout_height="36dp"
                            android:contentDescription="@string/cd"
                            android:src="@drawable/ic_reload"
                            android:visibility="gone"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:tint="@color/colorPrimary"
                            tools:visibility="visible" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="18dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/orderTrackingBtn"
                            android:layout_width="0dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginEnd="8dp"
                            android:layout_weight="1"
                            android:background="@drawable/bg_nearby_hub"
                            android:gravity="center"
                            android:orientation="horizontal"
                            tools:ignore="UselessParent">

                            <ImageView
                                android:layout_width="28dp"
                                android:layout_height="28dp"
                                android:contentDescription="@string/cd"
                                android:src="@drawable/ic_tracking"
                                app:tint="@color/white"
                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="8dp"
                                android:ellipsize="end"
                                android:maxLines="1"
                                android:text="ট্র্যাক অর্ডার"
                                android:textColor="@color/white"
                                android:textSize="16sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/complainBtn"
                            android:layout_width="0dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginStart="8dp"
                            android:layout_weight="1"
                            android:background="@drawable/bg_complain"
                            android:gravity="center"
                            android:orientation="horizontal"
                            tools:ignore="UselessParent">

                            <ImageView
                                android:layout_width="24dp"
                                android:layout_height="24dp"
                                android:contentDescription="@string/cd"
                                android:src="@drawable/ic_complain" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="8dp"
                                android:ellipsize="end"
                                android:maxLines="1"
                                android:text="কমপ্লেইন"
                                android:textColor="@color/white"
                                android:textSize="16sp" />

                        </LinearLayout>

                    </LinearLayout>

                    <com.smarteist.autoimageslider.SliderView
                        android:id="@+id/sliderView"
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="18dp"
                        android:visibility="gone"
                        app:sliderIndicatorMarginBottom="3dp"
                        app:sliderIndicatorRadius="2dp"
                        tools:visibility="visible"
                        tools:background="@color/black_15"
                        />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deliveredReturnLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:background="@drawable/bg_stroke1"
                        android:gravity="center"
                        android:orientation="horizontal"
                        tools:ignore="UselessParent"
                        tools:visibility="visible">

                        <TextView
                            android:id="@+id/titleDeliveryReturn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/title_oval_shape"
                            android:padding="5dp"
                            android:text="ডেলিভারি ও রিটার্ন ট্রেন্ড"
                            android:textSize="@dimen/text_14"
                            android:textStyle="bold"
                            android:textColor="@color/yellow"
                            android:textAlignment="center"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <View
                            android:id="@+id/separator"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/separator"
                            app:layout_constraintTop_toBottomOf="@id/titleDeliveryReturn" />

                        <LinearLayout
                            android:id="@+id/datePickerLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintTop_toBottomOf="@+id/separator"
                            app:layout_constraintEnd_toEndOf="parent"
                            tools:ignore="UseCompoundDrawables"
                            android:gravity="center_vertical"
                            >

                            <TextView
                                android:id="@+id/dateRangePicker"
                                android:layout_width="0dp"
                                android:layout_height="40dp"
                                android:layout_weight="1"
                                android:background="@drawable/bg_add_order_edit_text"
                                android:hint="ডেট রেঞ্জ সিলেক্ট করুন"
                                android:gravity="center_vertical"
                                android:includeFontPadding="false"
                                android:paddingStart="16dp"
                                android:paddingTop="8dp"
                                android:paddingEnd="16dp"
                                android:paddingBottom="8dp"
                                android:textColor="@color/black_80"
                                android:textColorHint="@color/red"
                                android:textSize="@dimen/text_14"
                                app:drawableEndCompat="@drawable/ic_date" />

                            <ImageView
                                android:id="@+id/clearDateRangeImage"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginStart="2dp"
                                android:layout_marginEnd="4dp"
                                android:layout_gravity="center"
                                android:src="@drawable/ic_close_white_24dp"
                                app:tint="@color/black_80"
                                android:contentDescription="@string/cd"
                                android:visibility="gone"
                                tools:visibility="gone"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginEnd="12dp"
                            android:layout_marginBottom="12dp"
                            android:orientation="horizontal"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/datePickerLayout"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:baselineAligned="false">

                            <LinearLayout
                                android:id="@+id/deliveryFilterLayout"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginEnd="8dp"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:paddingTop="6dp"
                                android:orientation="vertical"
                                tools:ignore="UselessParent">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="@drawable/bg_top_oval_shape"
                                    android:padding="5dp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/titleDelivery"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="end"
                                        android:maxLines="1"
                                        android:paddingEnd="4dp"
                                        android:text="ডেলিভারি হয়েছে"
                                        android:textColor="@color/white"
                                        android:textSize="14sp" />

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:layout_gravity="center_horizontal"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:maxLines="1"
                                            android:textColor="@color/white"
                                            android:textSize="@dimen/text_14"
                                            android:text="("/>

                                        <TextView
                                            android:id="@+id/filterCountDeliveryPercent"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:maxLines="1"
                                            android:textColor="@color/yellow"
                                            android:textSize="@dimen/text_14"
                                            android:text="-" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:maxLines="1"
                                            android:textColor="@color/yellow"
                                            android:textSize="@dimen/text_12"
                                            android:text="%" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:maxLines="1"
                                            android:textColor="@color/white"
                                            android:textSize="@dimen/text_14"
                                            android:text=")"/>

                                    </LinearLayout>

                                </LinearLayout>

                                <TextView
                                    android:id="@+id/filterCountDelivery"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:padding="5dp"
                                    android:gravity="center_horizontal"
                                    android:layout_weight="1"
                                    android:layout_gravity="center"
                                    android:background="@drawable/bg_bottom_oval_shape"
                                    android:maxLines="1"
                                    android:textSize="@dimen/text_16"
                                    android:textStyle="bold"
                                    android:text="-"
                                    tools:text="০টি"
                                    android:textColor="@color/black_80" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginStart="8dp"
                                android:layout_weight="1"
                                android:paddingTop="6dp"
                                android:gravity="center"
                                android:orientation="vertical"
                                tools:ignore="UselessParent">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_top_oval_shape"
                                    android:padding="5dp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/titleReturn"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="end"
                                        android:maxLines="1"
                                        android:paddingStart="8dp"
                                        android:paddingEnd="4dp"
                                        android:textColor="@color/white"
                                        android:textSize="14sp"
                                        android:text="রিটার্ন হয়েছে"/>

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:orientation="horizontal"
                                        android:layout_gravity="center_horizontal">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:maxLines="1"
                                            android:textColor="@color/white"
                                            android:textSize="@dimen/text_14"
                                            android:text="("
                                            />

                                        <TextView
                                            android:id="@+id/filterCountReturnPercent"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:maxLines="1"
                                            android:textColor="@color/yellow"
                                            android:textSize="@dimen/text_14"
                                            android:text="-"
                                            />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:maxLines="1"
                                            android:textColor="@color/yellow"
                                            android:textSize="@dimen/text_12"
                                            android:text="%"
                                            />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:maxLines="1"
                                            android:textColor="@color/white"
                                            android:textSize="@dimen/text_14"
                                            android:text=")"
                                            />

                                    </LinearLayout>

                                </LinearLayout>

                                <TextView
                                    android:id="@+id/filterCountReturn"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:background="@drawable/bg_bottom_oval_shape"
                                    android:layout_weight="1"
                                    android:padding="5dp"
                                    android:gravity="center_horizontal"
                                    android:maxLines="1"
                                    android:textSize="@dimen/text_16"
                                    android:textStyle="bold"
                                    android:text="-"
                                    tools:text="০টি"
                                    android:textColor="@color/black_80" />

                            </LinearLayout>

                        </LinearLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <ImageView
                        android:id="@+id/referBtn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:adjustViewBounds="true"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="18dp"
                        android:contentDescription="@string/cd"
                        android:src="@drawable/bg_banner_referral"
                        android:visibility="gone"
                        tools:visibility="visible"
                        />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:layout_marginBottom="16dp"
                        android:background="@drawable/bg_chat_qa_stroke"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:background="@drawable/bang_lay_pending_query"
                            android:gravity="center">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="আপনার একাউন্ট ম্যানেজার এর তথ্য"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="12dp">


                                <ImageView
                                    android:id="@+id/retentionManagerImage"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:contentDescription="@string/cd"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/ic_avater_demo"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    tools:src="@tools:sample/avatars" />

                                <TextView
                                    android:id="@+id/retentionManagerName"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="8dp"
                                    android:ellipsize="end"
                                    android:maxLines="1"
                                    android:textColor="@color/black_80"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    app:layout_constraintEnd_toStartOf="@+id/callBtn"
                                    app:layout_constraintStart_toEndOf="@id/retentionManagerImage"
                                    app:layout_constraintTop_toTopOf="parent"
                                    tools:text="Md Sifat" />

                                <TextView
                                    android:id="@+id/retentionManagerNumber"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="8dp"
                                    android:maxLines="1"
                                    android:textColor="@color/black_80"
