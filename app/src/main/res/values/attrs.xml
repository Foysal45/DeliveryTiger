<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SVGImageView">
        <!-- Location of the SVG document. -->
        <attr name="svg" format="reference|string" />
        <attr name="css" format="string" />
    </declare-styleable>
    <declare-styleable name="VerticalTextView">
        <attr name="direction">
            <enum name="uptodown" value="0" />
            <enum name="downtoup" value="1" />
            <enum name="lefttoright" value="2" />
            <enum name="righttoleft" value="3" />
        </attr>
    </declare-styleable>
</resources>